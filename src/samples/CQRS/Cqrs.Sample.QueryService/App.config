<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MsmqTransportConfig" type="NServiceBus.Config.MsmqTransportConfig, NServiceBus.Core"/>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>

  <MsmqTransportConfig InputQueue="CqrsSampleQueryServiceInputQueue" ErrorQueue="CqrsSampleError" NumberOfWorkerThreads="1" MaxRetries="5"/>
  <UnicastBusConfig>
    <MessageEndpointMappings>
      <add Messages="Cqrs.Sample.Events" Endpoint="CqrsSampleCommandServiceInputQueue"/>
    </MessageEndpointMappings>
  </UnicastBusConfig>

  <connectionStrings>
    <add name="ViewModelContext" providerName="System.Data.SqlClient"
			 connectionString="Data Source=.\sqlexpress;Initial Catalog=CqrsSampleViewModels;Integrated Security=SSPI;Enlist=false;" />
    <add name="CqrsSampleEventStore" providerName="System.Data.SqlClient"
			 connectionString="Data Source=.\sqlexpress;Initial Catalog=CqrsSampleEventStore;Integrated Security=SSPI;Enlist=false;" />
  </connectionStrings>
</configuration>